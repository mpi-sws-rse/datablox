[
  { "key": {"name":"file-dedup", "version":"1.0"},
    "display_name": "Resource for datablox block file-dedup",
    "input_ports": {
      "datablox_framework": {
        "BLOXPATH": "path"
      }
    },
    "output_ports": {
      "block_info": {
          "home":{
            "type":"path",
            "fixed-value":"${input_ports.datablox_framework.BLOXPATH}/file_dedup__1_0"
          }
      }
    },
    "inside": {
       "one-of": [
          { "key": {"name": "ubuntu-linux", "version":"11.10"}},
          { "key": {"name": "ubuntu-linux", "version":"10.04"}},
          { "key": {"name":"mac-osx", "version":{"greater-than-or-equal":"10.5",
                                                 "less-than":"10.7"}}}
        ]
     },
    "environment": {
      "all-of": [
        {"key": {"name":"datablox-framework", "version":"1.0"},
         "port_mapping": {"datablox_framework":"datablox_framework"}},
        {"key": {"name":"redis", "version":"2.4"}},
        {"key": {"name":"redis-py", "version":""}}
    ]}
  }
]
