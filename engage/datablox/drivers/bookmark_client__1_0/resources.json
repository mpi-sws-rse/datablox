[
  { "key": {"name":"bookmark-client", "version":"1.0"},
    "display_name": "Resource for datablox block bookmark-client",
    "input_ports": {
      "datablox_framework": {
        "BLOXPATH": "path"
      }
    },
    "output_ports": {
      "block_info": {
          "home":{
            "type":"path",
            "fixed-value":"${input_ports.datablox_framework.BLOXPATH}/bookmark_client__1_0"
          }
      }
    },
    "inside": {
       "one-of": [
          { "key": {"name": "ubuntu-linux", "version":"14.04"}},
          { "key": {"name": "ubuntu-linux", "version":"12.04"}},
          { "key": {"name":"mac-osx", "version":{"greater-than-or-equal":"10.5",
                                                 "less-than":"10.7"}}}
        ]
     },
    "environment": {
      "key": {"name":"datablox-framework", "version":"1.0"},
      "port_mapping": {"datablox_framework":"datablox_framework"}
    }
  }
]
